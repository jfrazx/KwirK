    "@status/abstract": "^1.0.0",
    "@status/codes": "^1.6.2",
    "bluebird": "^3.5.0",
    "hashish": "0.0.4",
    "lodash": "^4.17.20",
    "moment-timezone": "^0.5.32",
    "rxjs": "^6.6.3",
    "surrogate": "^1.3.5",
    "timerjobs": "^2.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/chai": "^4.2.14",
    "@types/eventemitter2": "^4.1.0",
    "@types/iconv": "^2.1.16",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.13",
    "@types/sinon": "^9.0.9",
    "@types/winston": "^2.4.4",
