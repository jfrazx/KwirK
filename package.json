{
  "name": "kwirk",
  "version": "0.2.11",
  "description": "KwirK, a sophisticated multi-network, multi-protocol bot, (eventually).",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc",
    "prebuild:examples": "rimraf build-examples && npm run build",
    "build:examples": "tsc -p ./examples/tsconfig.json",
